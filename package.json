{
  "name": "gd-playground",
  "version": "0.0.0-semantic-release",
  "description": "graph data playground",
  "keywords": [],
  "scripts": {
    "cover": "nyc ava",
    "doc": "jsdoc lib/*.js",
    "test": "ava",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "precover": "rollup -c tests/rollup.config.js",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "docs": "jsdoc2md --configure doc/jsdoc.json -l off -t doc/README.hbs -f src/*.js >README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/gd-playground.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/gd-playground/issues"
  },
  "homepage": "https://github.com/arlac77/gd-playground#readme",
  "dependencies": {
    "hdt": "^1.4.1",
    "level-ws": "0.1.0",
    "leveldown": "^1.8.0",
    "levelup": "^1.3.9"
  },
  "devDependencies": {
    "semantic-release": "^8.0.3",
    "jsdoc-to-markdown": "^3.0.0",
    "jsdoc-babel": "^0.3.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.2.1",
    "ava": "^0.22.0",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-multi-entry": "^2.0.1",
    "xo": "^0.19.0"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=8.5.0"
  },
  "main": "dist/index.js",
  "module": "src/index.js",
  "xo": {
    "space": true
  },
  "ava": {
    "files": [
      "build/test-bundle.js"
    ],
    "presets": [
      "stage-3"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/test-bundle.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
